{
  "type": "object",
  "description": "Triggered when a new user accepts the analytics prompt that is shown during the registration flow.",
  "properties": {
    "eventName": {
      "enum": ["Signup"]
    },
    "authenticationType": {
      "type": "string",
      "description": "The type of authentication that was used to sign up.",
      "oneOf": [
        {"const": "Password", "description": "Registration with a username and password."},
        {"const": "SignInWithApple", "description": "Social login using Apple."},
        {"const": "SignInWithFacebook", "description": "Social login using Facebook."},
        {"const": "SignInWithGitHub", "description": "Social login using GitHub."},
        {"const": "SignInWithGitLab", "description": "Social login using GitLab."},
        {"const": "SignInWithGoogle", "description": "Social login using Google."},
        {"const": "SSO", "description": "Registration using another SSO provider."}
      ],
      "required": true
    }
  },
  "required": ["eventName"],
  "additionalProperties": false
}
